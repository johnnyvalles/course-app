<%- include("../partials/header.ejs") %>
<div class="ui container">
    <h1 class="ui header">Enrolled Students</h1>
    <hr>
    <div class="ui grid cards">
        <% people.forEach((person) => { %>
        <div class="card">
            <div class="content">
                <img class="right floated ui image rounded" src="<%=person.img%>">
                <div class="header"><%= `${person.first} ${person.last}` %></div>
                <div class="description"><%= person.bio.substring(0, 50)%>...</div>
            </div>
            <a class="ui bottom primary attached button" href="/students/<%= person._id%>"><i class="eye icon"></i>view</a>
        </div>
        <% }) %>
    </div>
</div>
<%- include("../partials/footer.ejs") %>